# アプリケーション設定
NODE_ENV=development
PORT=3000

# データベース設定
DATABASE_URL="postgresql://[username]:[password]@localhost:5432/[database_name]?schema=public"

# NextAuth設定
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET="your-secret-key-at-least-32-chars"

# セッション設定
SESSION_SECRET=your-session-secret

# CORS設定
CORS_ORIGINS=http://localhost:3000

# ログ設定
LOG_LEVEL=debug

# 認証
NEXTAUTH_SECRET="your-nextauth-secret-key"
NEXTAUTH_URL="http://localhost:3000"

# GitHub OAuth
GITHUB_ID="your-github-id"
GITHUB_SECRET="your-github-secret"

# Supabase（ファイルストレージ用）
SUPABASE_URL="your-supabase-url"
SUPABASE_KEY="your-supabase-key"

# SkyWay設定（ボイスチャット用）
NEXT_PUBLIC_SKYWAY_APP_ID="your-skyway-app-id"
SKYWAY_SECRET_KEY="your-skyway-secret-key"

# 仮のデータ表示
NEXT_PUBLIC_USE_MOCK_DATA="false"

NEXT_PUBLIC_APP_URL=http://localhost:3000

SOCKET_PORT=3002

# GetStream Chat API Keys(https://getstream.io/)
NEXT_PUBLIC_STREAM_KEY=your-stream-key  # ダッシュボードのAPI Keyをコピー
STREAM_SECRET_KEY=your-stream-secret-key  # ダッシュボードのAPI Secretをコピー

# Admin User
ADMIN_EMAIL=your-admin-email
ADMIN_PASSWORD=your-admin-password

# Google OAuth
GOOGLE_CLIENT_ID=your_client_id_here
GOOGLE_CLIENT_SECRET=your_client_secret_here